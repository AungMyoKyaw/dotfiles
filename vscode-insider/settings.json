  /**
   * Exclude large, frequently changing, or binary folders from the file watcher.
   * This reduces CPU and memory usage by telling VS Code not to monitor these directories.
   */
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.hg/store/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/.next/**": true,
    "**/out/**": true,
    "**/coverage/**": true
  },

  /**
   * Exclude the same large directories from search operations to make workspace-wide searches faster.
   */
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/dist": true,
    "**/build": true,
    "**/.next": true,
    "**/out": true,
    "**/coverage": true
  },

  /**
