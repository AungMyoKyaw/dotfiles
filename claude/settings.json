{
  "env": {
    "CLAUDE_CODE_MAX_OUTPUT_TOKENS": "16384",
    "MAX_THINKING_TOKENS": "100000",
    "CLAUDE_BASH_MAINTAIN_PROJECT_WORKING_DIR": "2",
    "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": "1",
    "DISABLE_COST_WARNINGS": "1",
    "DISABLE_ERROR_REPORTING": "1",
    "DISABLE_TELEMETRY": "1",
    "NO_PROXY": "localhost,127.0.0.1,::1",
    "CLAUDE_CODE_CACHE_ENABLED": "1",
    "CLAUDE_CODE_PARALLEL_REQUESTS": "16",
    "UV_THREADPOOL_SIZE": "32",
    "CLAUDE_CODE_FAST_MODE": "1",
    "CLAUDE_BASH_NO_LOGIN": "1",
    "NODE_OPTIONS": "--max-old-space-size=8192 --max-semi-space-size=256",
    "USE_BUILTIN_RIPGREP": "1",
    "CLAUDE_CODE_AUTO_CONNECT_IDE": "0",
    "CLAUDE_CODE_API_KEY_HELPER_TTL_MS": "3600000",
    "BASH_DEFAULT_TIMEOUT_MS": "300000",
    "BASH_MAX_TIMEOUT_MS": "600000"
  },
  "includeCoAuthoredBy": false,
  "alwaysThinkingEnabled": true,
  "performance": {
    "enableParallelProcessing": true,
    "maxConcurrentTasks": 16,
    "cacheSize": "8GB",
    "prefetchEnabled": true,
    "compressionEnabled": false,
    "batchSize": 128
  },
  "enabledPlugins": {
    "superpowers@superpowers-marketplace": true
  }
}
